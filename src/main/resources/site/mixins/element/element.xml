<mixin>
    <display-name>Element</display-name>
    <items>
        <inline mixin="elementId"/>
        <field-set name="override">
            <label>Override/extend</label>
            <items>
                <input name="tag" type="TextLine">
                    <label>Tag</label>
                    <occurrences minimum="0" maximum="1"/>
                    <config/>
                </input>
                <inline mixin="content"/>
                <inline mixin="attributes"/>
                <inline mixin="style"/>
                <inline mixin="breakpoints"/>
            </items>
        </field-set>
    </items>
</mixin>
